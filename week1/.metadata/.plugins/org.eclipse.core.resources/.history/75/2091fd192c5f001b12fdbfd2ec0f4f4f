package com.revature;

public class Test2 {
	
	private final int x;
	
	{ x = 10; } // static initialization block
	
	// this would be initialized only once
	
	
	 // this will persist
	
	
	
	Test2() {
		System.out.println(x);
	}
	
	public static void main(String[] args) {
		Test2 obj = new Test2();
	}

}
